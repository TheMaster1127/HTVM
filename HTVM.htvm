func str extraFlexableFuncCalls(str line, str availableFuncsInHTVMInHTVM)
{
StringTrimRight, availableFuncsInHTVMInHTVM, availableFuncsInHTVMInHTVM, 1
str out := line

    arr str funcCalls
    str temp := ""
    int parenthesesDepth := 0 ; Track parentheses depth
    int seenDot := 0
    Loop, Parse, line
    {
    if (A_LoopField = "(")
    {
    seenDot := 1
    }
        ; Handle spaces
        if (A_LoopField = " ")
        {
            if (parenthesesDepth = 0) ; Split only outside parentheses
            {
                if (temp != "")
                {
                    funcCalls.add(temp)
                    temp := ""
                }
            }
            else
            {
                temp .= A_LoopField ; Inside parentheses, accumulate
            }
        }
        else if (A_LoopField = "(") and (seenDot = 1)
        {
            parenthesesDepth++ ; Increase depth for function argument list
            temp .= A_LoopField
        }
        else if (A_LoopField = ")") and (seenDot = 1)
        {
            parenthesesDepth-- ; Decrease depth after closing argument list
            temp .= A_LoopField
        }
        else
        {
            temp .= A_LoopField ; Accumulate characters for function name/args
        }
    }

    if (temp != "")
    {
        funcCalls.add(temp) ; Add the last accumulated segment
    }


